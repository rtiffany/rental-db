<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.rentaldb.controller.EquipmentController">
   <children>
      <VBox prefHeight="600.0" prefWidth="800.0">
         <children>
            <HBox prefHeight="75.0" prefWidth="800.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#onEquipmentSalesButtonClick" text="Equipment Sales" />
                  <Button mnemonicParsing="false" onAction="#onEmployeesButtonClick" text="Employees" />
               </children>
            </HBox>
            <TableView prefHeight="453.0" prefWidth="800.0">
              <columns>
                <TableColumn prefWidth="75.0" text="id" />
                <TableColumn prefWidth="75.0" text="make" />
                  <TableColumn prefWidth="75.0" text="year" />
                  <TableColumn prefWidth="75.0" text="cost" />
                  <TableColumn prefWidth="75.0" text="available" />
                  <TableColumn prefWidth="112.0" text="totalHours" />
              </columns>
            </TableView>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#onEquipmentAddButtonClick" text="Add" />
                  <Button mnemonicParsing="false" onAction="#onEquipmentEditButtonClick" text="Edit" />
                  <Button mnemonicParsing="false" onAction="#onEquipmentCheckInOutButtonClick" text="Check In/Check Out" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
